program Lab3Exp3;

{$APPTYPE CONSOLE}

{$R *.res}

uses
  System.SysUtils;

const
  N = 10;
Var Mas:array[1..N] of real = (1.1,6,-3.2,1.1,9,6,7,-3.2,9,1.1);
    i, j, t, m: integer;
Begin
  writeln('Исходный массив');
  for i:=1 to N do
  write(Mas[i]:6:2);
  m:=N;//запоминили исходный размер массива
  i:=0;
  while i<m do
  begin
    inc(i);
    j:=i;
    While j<m do
    begin
      inc(j);
      if Mas[i]=Mas[j] then //если нашли повторяющийся то
      begin
        For t:=j to m-1 do
        Mas[t]:=Mas[t+1];//сдвигаем элементы
        dec(m);//уменьшаем размер массива
        j := i;
      end;
    end;
  end;
  writeln;
  writeln('Преобразованный массив');
  for i:=1 to m do
  write(Mas[i]:6:2);
  readln;
End.
