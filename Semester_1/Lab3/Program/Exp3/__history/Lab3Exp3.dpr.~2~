program Lab3Exp3;
//Program exclude repeating elements

//Console app
{$APPTYPE CONSOLE}

{$R *.res}

//Modules declaration
uses
  System.SysUtils;

//Constants declaration
const
  N = 10;

//Variables declaration
Var Mas:array[1..N] of real = (1.1,6,-3.2,1.1,9,6,7,-3.2,9,1.1);
    i, j, k, Size: integer;
  { Mas - array
    i - counter 1
    j - counter 2
    k - counter 3
    Size - current size of array }

//Start the program
Begin

  //Output original array
  writeln('Исходный массив');
  for i:=1 to N do
  write(Mas[i]:6:2);

  //Remember the original size of array
  Size:=N;

  //Set i to the original value
  i:=0;

  //Entering the cycle
  while i < Size do
  begin

    //Increment i
    inc(i);

    //Make j equals to i
    j:=i;

    //Entering the cycle
    While j<Size do
    begin

      //Increment of j
      inc(j);

      //If repeated element was found
      if Mas[i]=Mas[j] then
      begin
        For k:=j to Size-1 do

        //Move elements to the left
        Mas[k]:=Mas[k+1];

        //Decrease size of array
        dec(Size);

        //If number repeated more than once
        j := i;
      end;
    end;
  end;
  writeln;

  //Output new array
  writeln('Преобразованный массив');
  for i:=1 to Size do
  write(Mas[i]:6:2);

  //Final operations
  readln;

End.
