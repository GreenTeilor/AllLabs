program Exp1;

{$APPTYPE CONSOLE}

{$R *.res}

uses
  System.SysUtils;

var
  S, tmp, buf, firstW: string;
  i, j, k, p, ii: integer;
  ch: char;

begin
  S := 'privit,kak dela putin,znanija,tet,proverka, pr , tet.';
  i := length(S) - 1;
  while (S[i] <> ',') and (S[i] <> ' ') do
  begin
    firstW := firstW + copy(S,i,1);
    dec(i);
  end;

  writeln('Исходная строка: ', S);
  writeln('Последнее слово = ', firstW);
  i := 1;
  while (S[i] <> '.') do
  begin
    if (S[i] <> ' ') and (S[i] <> ',') then
    begin
      while (S[i] <> ' ') and (S[i] <> ',') and (i < Length(S)) do
      begin
        tmp := tmp + S[i];
        i := i + 1;
      end;
      buf := tmp;
      p := 0;
      ii := 1;
      while ii <= Length(buf) do
      begin
        k := 0;
        ch := buf[ii];
        for j := Length(buf) downto ii do
          if buf[j] = ch then
          begin
            k := k + 1;
            delete(buf, j, 1);
          end;
        if k > 1 then
          p := 1;
      end;
      if ((p <> 0) and (tmp <> firstW)) then
        writeln(tmp);
      tmp := '';
    end;
    i := i + 1;
  end;
  readln;
end.
